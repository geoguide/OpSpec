{
  "extends": [
    "rallycoding"
  ],
  "parser": "babel-eslint",
  "globals": { "fetch": false },
  "rules": {
      "arrow-body-style": 0,
      "no-unused-vars": 1,
      "comma-spacing": 1,
      "object-curly-spacing": 1,
      "space-before-blocks": 1,
      "camelcase": 0,
      "keyword-spacing": 0,
      "prefer-const": 1,
      "max-len": 1
  },
  "plugins": [
  ]
}
