{
  "extends": [

  ],
  "parser": "babel-eslint",
  "globals": { "fetch": false },
  "rules": {
      "max-len": 0,
      "arrow-body-style": 0,
      "no-unused-vars": 1,
      "comma-spacing": 1,
      "object-curly-spacing": ["warn", "always"],
      "space-before-blocks": 1,
      "camelcase": 0,
      "keyword-spacing": 0,
      "prefer-const": 1,
      "eqeqeq": 2,
      "no-extra-bind": 1,
      "no-loop-func": 1,
      "no-multi-spaces": 1,
      "no-redeclare": 1,
      "block-spacing": ["warn", "always"],
      "no-lonely-if": 2,
      "no-whitespace-before-property": 2
  },
  "plugins": [
  ]
}
